// Definice funkce pro vypocet hodnoty faktorialu
function factorial($n)
{
	if ($n < 2)
	{
		$result = 1;
	}
	else
	{
		$fuckOff = "nejakej string pri zanorovani";
		$decremented_n = $n - 1;
		$temp_result = factorial($decremented_n);
		$result = $n * $temp_result;
	}
	return $result;
}


$a = 11;
$vysl = factorial($a);
$z = put_string($vysl);

